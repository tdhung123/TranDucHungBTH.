<h1>Danh sách sinh viên</h1>
<br>
<table class="table border" >
    <tr>
        <th>
            <label> StudentID</label>
        </th>
        <th>
            <label> StudentName</label>
        </th>
        <th>
            <label> Age</label>
        </th>
    </tr>
    @foreach (var item in ViewData["Student"] as List<Student>)
        {
            <tr>
            <td>@item.StudentID</td>
            <td>@item.StudentName</td>
            <td>@item.Age</td>
            </tr>
        }
</table>
